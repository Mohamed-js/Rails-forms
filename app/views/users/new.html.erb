<h1> Hey, user! </h1>

<%= form_with model: @user, method: :post do |form| %>
  <%= form.label :username, "Username" %>
  <%= form.text_field :username %>
  <%= form.label :email, "Email" %>
  <%= form.email_field :email %>
  <%= form.label :password, "Password" %>
  <%= form.password_field :password %>
  <%= form.submit "Sign Up" %>
<% end %>